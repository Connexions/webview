            "questions": [
                {
                    "stimulus_html": "",
                    "stem_html": "<p>A spherical conductor has a net positive charge. Describe how the charge is distributed around the sphere's surface.</p>",
                    "answers": [
                        {
                            "content_html": "<p>All positive charge is on the sphere surface, and all negative charge is at the center.</p>"
                        },
                        {
                            "content_html": "<p>Positive and negative charge is spread equally around the sphere's surface.</p>"
                        },
                        {
                            "content_html": "<p>The net positive charge is spread evenly throughout the sphere's volume.</p>"
                        },
                        {
                            "content_html": "<p>The net positive charge is spread evenly around the sphere's surface.</p>"
                        }
                    ],
